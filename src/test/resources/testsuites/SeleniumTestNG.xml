<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="Suite1" parallel="tests" thread-count="2">

    <test name="Cross Browser with Selenium - ChromeDriver">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="test.TestNG_Scripts.SimpleCrossBrowserSearchTest"/>
        </classes>
    </test>

    <test name="Cross Browser with Selenium - FirefoxDriver">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="test.TestNG_Scripts.SimpleCrossBrowserSearchTest"/>
        </classes>
    </test>
</suite>